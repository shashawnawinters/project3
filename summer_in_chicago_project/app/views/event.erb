<div class="container mid-section">
  <div class="row">
    <div class="col-12 centered-text">
      <h2>Your guide to Summer Time Chicago free events!</h2>
      <h4><%= message %></h4>
    </div>
  </div>
</div>


<main class="container">
  <div>
    <form method="get" action="/#" accept-charset="utf-8">
	   <input type="search" name="eventsearch" placeholder="Search..."/>
	   <button class="search" type="submit" name="submit">Search</button>
    </form>
  </div>
  <div class="row">
    <div class="col-12 centered-text">
      <h2>Feautured Free Events In Chicago</h2>
      <div class="results-li"></div>
      <div id="results"></div>
    </div>
  </div>

</main>


